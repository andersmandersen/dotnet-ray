<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>Velcon.Ray</RootNamespace>
    <AssemblyName>Velcon.Ray</AssemblyName>
    <PackageOutputPath>./nupkg</PackageOutputPath>
    <PackageVersion>0.1.1</PackageVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>Debugging .Net with Ray to fix problems faster</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="PieroDeTomi.DotNet.WebUtils" Version="1.0.0" />
    <PackageReference Include="RestSharp" Version="106.11.7" />
  </ItemGroup>
</Project>
